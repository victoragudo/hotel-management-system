{
  "rabbit_version": "3.13",
  "users": [
    {
      "name": "rabbitmq",
      "password": "rabbitmq",
      "tags": "administrator"
    }
  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "rabbitmq",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "exchanges": [
    {
      "name": "hotel_exchange",
      "vhost": "/",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "queues": [
    {
      "name": "hotel_jobs",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-dead-letter-exchange": "",
        "x-dead-letter-routing-key": "hotel_jobs_dlq",
        "x-max-priority": 10
      }
    },
    {
      "name": "hotel_jobs_dlq",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "hotel_exchange",
      "vhost": "/",
      "destination": "hotel_jobs",
      "destination_type": "queue",
      "routing_key": "hotel.job",
      "arguments": {}
    }
  ]
}